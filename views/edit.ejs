<%- include('./partials/header.ejs') %>

    <h1>UPDATE REWIEW</h1>
    <form action="/noodle/<%= noodle.id %>?_method=PUT" method="POST">
      Name: <input
      type="text"
      name="name"
      value="<%= noodle.name %>"
      >
      <br>
      Restaurant: <input
      type="text"
      name="restaurant"
      value="<%= noodle.restaurant %>"
      >
      <br>
      Description: <input
      type="text"
      name="description"
      value="<%= noodle.description %>"
      >
      <br>
      Img: <input
      type="text"
      name="img"
      value="<%= noodle.img %>"
      >
      <br>
      Price: <input
      type="text"
      name="price"
      value="<%= noodle.price %>"
      >
      <br>
      Rating: <input
      type="text"
      name="rating"
      value="<%= noodle.rating %>"
      >
      <br>
      Review: <input
      type="text"
      name="review"
      value="<%= noodle.review %>">
      <br>
      <input type="submit" value="Update">
    </form>
    <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcQdkm9-4LaktdAntkqmM7C8Lia_3sPEQN_OkA&usqp=CAU" alt="">
  </body>
</html>
